{
  "name": "Rollup",
  "rollups": [
    {
      "save_as": "kairosdb.http.query_time_rollup",
      "query": {
        "cache_time": 0,
        "start_relative": {
          "value": "1",
          "unit": "hours"
        },
        "metrics": [
          {
            "name": "kairosdb.http.query_time",
            "limit": 10000,
            "tags": {
              "host": [
                "foo",
                "bar"
              ],
              "customer": [
                "foobar"
              ]
            },
            "aggregators": [
              {
                "name": "sum",
                "sampling": {
                  "value": 10,
                  "unit": "minutes"
                }
              },
              {
                "name": "avg",
                "sampling": {
                  "value": 10,
                  "unit": "hours"
                }
              }
            ]
          }
        ]
      }
    }
  ]
}